{
  "name": "@sotatek/avabot-referral-sc",
  "description": "Avabot referral contract",
  "version": "1.0.1",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "dependencies": {
    "@ethersproject/abi": "5.7.0",
    "@ethersproject/providers": "5.7.2",
    "ethers": "5.7.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "1.0.8",
    "@nomicfoundation/hardhat-toolbox": "2.0.2",
    "@nomiclabs/hardhat-ethers": "2.2.3",
    "@nomiclabs/hardhat-etherscan": "3.1.7",
    "@openzeppelin/contracts": "^4.9.3",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "@openzeppelin/hardhat-upgrades": "^3.0.1",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@typechain/ethers-v5": "10.2.1",
    "@typechain/hardhat": "6.1.6",
    "@types/chai": "4.3.5",
    "@types/fs-extra": "11.0.1",
    "@types/mocha": "10.0.1",
    "@types/node": "20.5.6",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "chai": "4.3.8",
    "cross-env": "7.0.3",
    "dotenv": "16.3.1",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "fs-extra": "11.1.1",
    "gitpkg": "1.0.0-beta.4",
    "hardhat": "2.18.3",
    "hardhat-deploy": "0.11.37",
    "hardhat-gas-reporter": "1.0.9",
    "lodash": "4.17.21",
    "mocha": "10.2.0",
    "prettier": "2.8.8",
    "prettier-plugin-solidity": "1.1.3",
    "rimraf": "5.0.1",
    "solhint": "3.6.2",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "0.8.4",
    "ts-generator": "0.1.1",
    "ts-node": "10.9.1",
    "typechain": "8.3.1",
    "typescript": "5.2.2"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "blockchain",
    "ethers",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity",
    "template",
    "typescript",
    "typechain"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf ./artifacts ./cache ./coverage ./types ./coverage.json && pnpm typechain",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"test/**/*.ts\" && pnpm typechain",
    "deploy:bscTestnet": "hardhat deploy --network bscTestnet",
    "deploy:blastSepolia": "hardhat deploy --network blastSepolia",
    "lint": "pnpm lint:sol && pnpm lint:ts && pnpm prettier:check",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --ignore-path ./.eslintignore --ext .js,.ts .",
    "prettier:check": "prettier --check \"**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\"",
    "test": "hardhat test",
    "typechain": "cross-env DOTENV_CONFIG_PATH=./.env.example TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "build": "npm run build:cjs && npm run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json"
  },
  "gitpkg": {
    "registry": "git@github.com:sotatek-dev/gitpkg-registry.git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sotatek-dev/avabot-referral-sc.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sotatek-dev/avabot-referral-sc/issues"
  },
  "homepage": "https://github.com/sotatek-dev/avabot-referral-sc#readme"
}
